{
  "expo": {
    "name": "Dho",
    "slug": "dho",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.saulgoofman.Ctime"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/new-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.saulgoofman.Ctime"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos.",
          "cameraPermission": "The app accesses your camera."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "12439da7-04b9-4adc-9a48-470a10215435"
      }
    }
  }
}
